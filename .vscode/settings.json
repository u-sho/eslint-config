{
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": true,
  "eslint.enable": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.probe": [
    "json",
    "jsonc",
    "javascript",
    "typescript",
    "markdown"
  ],
  "eslint.trace.server": "messages"
}